{
    "name": "directus-extension-yandex-foundation-models",
    "icon": "extension",
    "version": "0.0.1",
    "author": "garin1221@yandex.ru <Aleksandr Garin>",
    "license": "MIT",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-extension-bundle"
    ],
    "type": "module",
    "files": [
        "dist"
    ],
    "directus:extension": {
        "type": "bundle",
        "path": {
            "app": "dist/app.js",
            "api": "dist/api.js"
        },
        "entries": [
            {
                "type": "operation",
                "name": "yandex-gpt-operation",
                "source": {
                    "app": "src/yandex-gpt-operation/app.js",
                    "api": "src/yandex-gpt-operation/api.js"
                }
            }
        ],
        "host": "^10.10.0"
    },
    "scripts": {
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify",
        "link": "directus-extension link",
        "add": "directus-extension add"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "^9.18.1",
        "prettier": "^3.3.3"
    },
    "dependencies": {
        "node-fetch": "^3.3.2"
    }
}